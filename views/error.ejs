<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('partials/head.ejs'); %>
        <title>Private Message Board - Error</title>
    </head>
    
<body>
    <nav>
        <ul>
            <li class="nav-link">
                <a href="/">Home</a>
            </li>
        </ul>
    </nav>
    <main>
        <h1>
            <%= message %>
        </h1>   
        <h2>
            <%= error.status %>
        </h2>  
        <pre>
            <%= error.stack %>
        </pre> 
    </main>
    <footer>
        <%- include("partials/footer.ejs") %>
    </footer>
</body>
</html>

block content 
    h1= message 
    h2= error.status 
    pre #{error.stack}